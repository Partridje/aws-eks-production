# Example terraform.tfvars file
# Copy this file to terraform.tfvars and update with your values

aws_region = "eu-west-1"
vpc_cidr   = "10.0.0.0/16"

cluster_version = "1.30"

# IMPORTANT: Get your Route53 hosted zone ID for devseatit.com
# Run: aws route53 list-hosted-zones --query "HostedZones[?Name=='devseatit.com.'].Id" --output text
route53_zone_id = "Z0123456789ABC" # Replace with actual zone ID

# Restrict to your IP in production
cluster_endpoint_public_access_cidrs = ["0.0.0.0/0"]

alert_email = "tcytcerov@gmail.com"
